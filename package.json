{
  "name": "lynsite2",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "convex dev --once && concurrently -r bun:dev:web bun:dev:convex",
    "dev:web": "vite dev --host",
    "dev:ts": "tsc -b -w",
    "dev:convex": "convex dev",
    "build": "vite build && tsc --noEmit",
    "start": "HOST=0.0.0.0 bun .output/server/index.mjs",
    "format": "prettier --write .",
    "lint": "tsc && eslint .  --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@convex-dev/react-query": "^0.0.0-alpha.11",
    "@convex-dev/sharded-counter": "^0.1.8",
    "@fontsource-variable/bricolage-grotesque": "^5.2.10",
    "@fontsource/poppins": "^5.2.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/nitro-v2-vite-plugin": "^1.133.19",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-router": "^1.132.2",
    "@tanstack/react-router-with-query": "^1.130.17",
    "@tanstack/react-start": "^1.132.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.28.0",
    "lucide-react": "^0.548.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@convex-dev/eslint-plugin": "^1.0.0",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/eslint-config": "^0.3.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "concurrently": "~9.2.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "bun@1.2.19",
  "engines": {
    "node": "22"
  }
}
